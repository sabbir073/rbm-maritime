{
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(find:*)",
      "Bash(if exist node_modules rmdir /s /q node_modules)",
      "Bash(if exist package-lock.json del package-lock.json)",
      "Bash(if exist .next rmdir /s /q .next)",
      "Bash(npm install:*)",
      "Bash(npm run build:*)"
    ],
    "deny": [],
    "ask": []
  }
}
